<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="384dp"
        android:layout_height="52dp"
        android:background="?attr/colorPrimary"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/spinner_fooddic" />

    <EditText
        android:id="@+id/SearchText"
        android:layout_width="250dp"
        android:layout_height="34dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/wrap_whitebox"
        android:ems="10"
        android:hint="@string/SearchHint"
        android:inputType="text"
        android:paddingLeft="10dp"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="@+id/imageView29"
        app:layout_constraintLeft_toLeftOf="@+id/button_fooddic_cancel"
        app:layout_constraintTop_toTopOf="@+id/imageView29" />

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar10"
        android:layout_width="384dp"
        android:layout_height="56dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_fooddic_cancel"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="88dp"
        android:layout_height="50dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/Cancel"
        android:textColor="@android:color/white"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/spinner_fooddic" />

    <Button
        android:id="@+id/button_fooddic_myfood"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="88dp"
        android:layout_height="50dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:text="@string/MyFood"
        android:textColor="@android:color/white"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/spinner_fooddic" />

    <Spinner
        android:id="@+id/spinner_fooddic"
        android:layout_width="182dp"
        android:layout_height="50dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@android:color/white"
        android:entries="@array/android_dropdown_arrays2"
        android:padding="5dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar10"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/button_fooddic_cancel"
        app:layout_constraintRight_toLeftOf="@+id/button_fooddic_myfood"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_fooddic_search"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="9dp"
        android:background="@android:drawable/ic_menu_search"
        app:layout_constraintRight_toLeftOf="@+id/button_fooddic_makemy"
        app:layout_constraintTop_toTopOf="@+id/imageView29"
        android:layout_marginRight="6dp" />

    <Button
        android:id="@+id/button_fooddic_camera"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="9dp"
        android:background="@android:drawable/ic_menu_camera"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView29" />

    <Button
        android:id="@+id/button_fooddic_makemy"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="9dp"
        android:background="@android:drawable/ic_menu_edit"
        app:layout_constraintRight_toLeftOf="@+id/button_fooddic_camera"
        app:layout_constraintTop_toTopOf="@+id/imageView29"
        android:layout_marginRight="6dp" />

    <fragment
        android:id="@+id/fragment"
        android:name="android.support.v4.app.ListFragment"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView29"
        android:layout_marginLeft="8dp"
        app:layout_constraintVertical_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
