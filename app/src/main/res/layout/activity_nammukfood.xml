        android:textColor="#ffffff"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        android:layout_marginBottom="12dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView7"
        app:layout_constraintRight_toRightOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.333"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="74dp"
        android:text="@string/CurrentExp"
        android:textColor="#ffffff"
        android:textStyle="bold"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/imageView7"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="74dp"
        app:layout_constraintRight_toRightOf="@+id/imageView6"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/user_class"
        android:layout_marginTop="72dp"
        app:layout_constraintTop_toBottomOf="@+id/button_change_nammukfood"
        android:layout_marginBottom="72dp" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="@android:style/Widget.DeviceDefault.Light.ProgressBar.Horizontal"
        android:layout_width="150dp"
        android:layout_height="33dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="10dp"
        android:max="100"
        android:progress="56"
        android:progressTint="@android:color/holo_orange_light"
        android:scaleY="7"
        app:layout_constraintLeft_toLeftOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/user_achievecount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="7dp"
        android:text="@string/AchieveCount"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/progressBar2"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/user_achievecounttotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="9dp"
        android:layout_marginStart="9dp"
        android:text="@string/UserAchieveCountTotal"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/user_achievecount"
        app:layout_constraintLeft_toRightOf="@+id/user_achievecount" />

    <TextView
        android:id="@+id/user_achieverpercent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="8dp"
        android:text="@string/UserAchievePercent"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:layout_constraintRight_toLeftOf="@+id/user_achievecounttotal"
        app:layout_constraintTop_toTopOf="@+id/progressBar2"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/user_achievecountcurrent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/UserAchieveCountCurrent"
        android:textColor="#ffffff"
        android:textStyle="bold"
        app:layout_constraintBaseline_toBaselineOf="@+id/user_achieverpercent"
        app:layout_constraintLeft_toRightOf="@+id/user_achievecounttotal"
        app:layout_constraintRight_toRightOf="@+id/user_achievecounttotal"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="8dp"
        android:text="@string/ClassInfo"
        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintLeft_toRightOf="@+id/imageView7"
        app:layout_constraintRight_toRightOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="10dp" />

    <ProgressBar
        android:id="@+id/progressBar5"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="0dp"
        android:layout_height="33dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:max="100"
        android:progress="50"
        android:progressTint="@android:color/holo_orange_light"
        android:scaleY="7"
        app:layout_constraintLeft_toLeftOf="@+id/imageView5"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/myinfo_sex"
        android:layout_marginTop="12dp"
        app:layout_constraintHorizontal_bias="0.531"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/KcalProgress"
        android:textColor="@android:color/background_light"
        app:layout_constraintBottom_toBottomOf="@+id/progressBar5"
        app:layout_constraintLeft_toLeftOf="@+id/progressBar5"
        app:layout_constraintRight_toRightOf="@+id/progressBar5"
        app:layout_constraintTop_toTopOf="@+id/progressBar5"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/burger"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        android:layout_marginBottom="8dp"
        app:layout_constraintRight_toRightOf="@+id/imageView9"
        app:layout_constraintLeft_toLeftOf="@+id/imageView9"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="133dp"
        android:layout_height="119dp"
        app:srcCompat="@mipmap/balloon"
        android:layout_marginLeft="20dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView5"
        android:layout_marginTop="54dp"
        app:layout_constraintTop_toBottomOf="@+id/textView24"
        android:layout_marginStart="20dp" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="37dp"
        android:layout_marginRight="39dp"
        android:layout_marginTop="53dp"
        android:text="@string/Brand"
        android:textColor="@android:color/background_light"
        android:textSize="20sp"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView24"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="37dp"
        android:layout_marginEnd="39dp"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="34dp"
        android:layout_marginRight="34dp"
        android:layout_marginTop="8dp"
        android:text="@string/FName"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="34dp"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="52dp"
        android:layout_marginRight="49dp"
        android:layout_marginTop="28dp"
        android:text="@string/KcalPerOne"
        android:textColor="@android:color/background_light"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="52dp"
        android:layout_marginEnd="49dp"
        tools:layout_constraintLeft_creator="1" />

    <Button
        android:id="@+id/button_change_nammukfood"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="90dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="53dp"
        android:layout_marginLeft="54dp"
        android:layout_marginRight="53dp"
        android:layout_marginStart="54dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/wrap_whiteborder"
        android:text="@string/Change"
        android:textColor="@android:color/background_light"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintLeft_toRightOf="@+id/imageView9"
        app:layout_constraintRight_toRightOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/textView33"
        app:layout_constraintVertical_bias="0.416"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/nammukpig"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView5"
        app:layout_constraintRight_toLeftOf="@+id/button_change_nammukfood"
        android:layout_marginRight="66dp"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="67dp"
        android:layout_marginEnd="66dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginBottom="-1dp" />

