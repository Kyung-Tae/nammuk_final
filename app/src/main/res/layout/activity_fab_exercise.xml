<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar7"
        android:layout_width="360dp"
        android:layout_height="56dp"
        android:background="?attr/colorPrimary"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/button_fabexercise_home"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@mipmap/home"
        android:backgroundTint="@android:color/white"
        android:textColor="@android:color/white"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar7"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="@string/fabexerciseToday"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@+id/toolbar7"
        app:layout_constraintRight_toRightOf="@+id/toolbar7"
        app:layout_constraintTop_toTopOf="@+id/toolbar7" />

    <Button
        android:id="@+id/button_fabexercise_calendar"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp"
        android:background="@mipmap/time"
        android:backgroundTint="@android:color/white"
        app:layout_constraintRight_toRightOf="@+id/toolbar7"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:background="#ff6161"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/fabexerciseTotal"
        android:textColor="@android:color/white"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView23"
        app:layout_constraintLeft_toLeftOf="@+id/imageView23"
        app:layout_constraintTop_toTopOf="@+id/imageView23" />

    <EditText
        android:id="@+id/editText_fabfood_total"
        android:layout_width="180dp"
        android:layout_height="35dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/wrap_whitebox"
        android:ems="10"
        android:gravity="end"

        android:inputType="number"
        android:textColor="#ff6161"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="@+id/imageView23"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView23"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="@+id/imageView23" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginTop="180dp"
        android:background="#ff6161"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView25" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:background="#ff6161"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbar7" />

    <TextView
        android:id="@+id/textView61"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/fabexerciseAerobic"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@+id/imageView26"
        app:layout_constraintBottom_toBottomOf="@+id/imageView26"
        android:layout_marginBottom="8dp" />

    <TextView
        android:id="@+id/textView62"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="11dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:text="@string/fabexerciseAnaerobic"
        android:textColor="@android:color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/listView2"
        app:layout_constraintLeft_toLeftOf="@+id/imageView25" />

    <EditText
        android:id="@+id/editText_fabexercise_anaerobic"
        android:layout_width="180dp"
        android:layout_height="35dp"

        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/wrap_whitebox"
        android:ems="10"
        android:gravity="end"
        android:inputType="number"
        android:textColor="#ff6161"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView25"
        app:layout_constraintTop_toTopOf="@+id/imageView25"
        app:layout_constraintRight_toLeftOf="@+id/button_fabexercise_anaerobicreg"
        android:layout_marginRight="8dp" />

    <EditText
        android:id="@+id/editText_fabexercise_aerobic"
        android:layout_width="180dp"
        android:layout_height="35dp"
        android:layout_marginBottom="8dp"

        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/wrap_whitebox"
        android:ems="10"
        android:gravity="end"
        android:inputType="number"
        android:textColor="#ff6161"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageView26"
        app:layout_constraintTop_toTopOf="@+id/imageView26"
        app:layout_constraintRight_toLeftOf="@+id/button_fabexercise_aerobicreg"
        android:layout_marginRight="8dp" />

    <Button
        android:id="@+id/button_fabexercise_anaerobicreg"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@mipmap/plusblack3"
        app:layout_constraintBottom_toBottomOf="@+id/imageView25"
        app:layout_constraintRight_toRightOf="@+id/imageView25"
        app:layout_constraintTop_toTopOf="@+id/imageView25"
        app:layout_constraintVertical_bias="0.4" />

    <Button
        android:id="@+id/button_fabexercise_aerobicreg"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@mipmap/plusblack3"
        app:layout_constraintBottom_toBottomOf="@+id/imageView26"
        app:layout_constraintRight_toRightOf="@+id/imageView26"
        app:layout_constraintTop_toTopOf="@+id/imageView26"
        app:layout_constraintVertical_bias="0.533" />

    <ListView
        android:layout_width="280dp"
        android:layout_height="62dp"
        android:id="@+id/listView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView25"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ListView
        android:layout_width="280dp"
        android:layout_height="62dp"
        android:id="@+id/listView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView26"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView23"
        app:layout_constraintVertical_bias="0.0" />

</android.support.constraint.ConstraintLayout>
